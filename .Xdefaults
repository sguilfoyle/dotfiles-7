! New colorset

*foreground: #e3e3e3
*background: #171717

 !black
*color0: #353535
*color8: #353535
!red    
*color1:    #B24747
*color9:    #B24747
!green
*color2:    #47B247
*color10:   #47B247
!yellow
*color3:    #B2B247
*color11:   #B2B247
!blue
*color4:    #476BB2
*color12:   #476BB2
!magenta    
*color5:    #B247B2
*color13:   #B247B2
!cyan
*color6:    #47B2B2
*color14:   #47B2B2
!white
*color7:    #AAAAAA
*color15:   #AAAAAA

URxvt*font: xft:lime:pixelsize=6:antialias=true:hinting=true
URxvt*boldFont: xft:lime:pixelsize=6:antialias=true:hinting:true
!URxvt*font: xft:Fixed:pixelsize=9
!URxvt*boldFont: xft:Fixed:pixelsize=9

URxvt*buffered: true

URxvt*cursorBlink: false
URxvt*cursorUnderline: false

URxvt*geometry: 75x15
URxvt*internalBorder: 3

!URxvt*reverseVideo: true

URxvt*depth: 24

URxvt*scrollBar: false
URxvt*scrollBar_right: true
URxvt*scrollstyle: plain
URxvt*scrollColor: #2873a2
URxvt*scrollWithBuffer: false
URxvt*scrollTtyOutput: true

URxvt*urgentOnBell: true
!URxvt*visualBell: true

URxvt*pointerBlank: true
