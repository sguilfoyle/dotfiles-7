! New colorset

*foreground: #808080
*background: #151515


!black
*color0: #353535
*color8: #353535
!red
*color1: #ba4d4d
*color9: #984040
!green
*color2: #bdf271
*color10: #ffffa6
!yellow
*color3: #d7cd94
*color11: #b3ab7c
!blue
*color4: #66AABB
*color12: #508a98
!magenta
*color5: #8e6a80
*color13: #745568
!cyan
*color6: #989081
*color14: #827c6f
!white
*color7: #666666
*color15: #cccccc


URxvt*font: xft:lime:pixelsize=6:antialias=false:hinting=false
URxvt*boldFont: xft:lime:pixelsize=6:antialias=false:hinting:false
!URxvt*font: xft:Fixed:pixelsize=9
!URxvt*boldFont: xft:Fixed:pixelsize=9

URxvt*buffered: true

URxvt*cursorBlink: false
URxvt*cursorUnderline: false

URxvt*geometry: 75x15
URxvt*internalBorder: 3

!URxvt*reverseVideo: true

URxvt*depth: 24

URxvt*scrollBar: false
URxvt*scrollBar_right: true
URxvt*scrollstyle: plain
URxvt*scrollColor: #2873a2
URxvt*scrollWithBuffer: false
URxvt*scrollTtyOutput: true

URxvt*urgentOnBell: true
!URxvt*visualBell: true

URxvt*pointerBlank: true
